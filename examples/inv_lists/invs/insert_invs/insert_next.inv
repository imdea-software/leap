param:

tid i

invariant [insertNext] :



    @head_next_diff(i). -> rd(heap, head).next != head
/\  @prev_is_head(i). -> insert::prev(i) = head
/\  @diff(i). -> insert::prev(i) != insert::curr(i)
/\  (@follows(i). /\ ~ @equals(i).) -> (insert::prev(i) != insert::curr(i) /\
                                         rd(heap,insert::prev(i)).next = insert::curr(i))
/\  @equals(i). -> insert::prev(i) = insert::curr(i)

/\  @inside_while(i). -> insert::curr(i) != null



