vars:

addr head
mem heap
mem heap_prime
int insert_i_k_0
addr insert_newCell_k_0
cell c
cell c_prime
addrarr insert_update_k_0
addrarr a
addrarr a_prime
addrSet region
addrSet region_prime
tidarr t
int m
elem e

formula:


//antecedent:
((~ insert_newCell_k_0 in addr2set(heap,head,insert_i_k_0 + 1) ) /\
   c       = rd(heap,insert_update_k_0[insert_i_k_0]) /\
   c       = mkcell(e,a,t,m)  /\
   c_prime = mkcell(e,a_prime,t,m) /\
   a_prime = arrUpd(a, insert_i_k_0, insert_newCell_k_0) /\
   heap_prime = upd(heap,insert_update_k_0[insert_i_k_0],c_prime) /\
   region_prime = Union(region,{ insert_newCell_k_0 })
)
->
//consequent:
(~ insert_newCell_k_0 in addr2set(heap_prime,head,insert_i_k_0 + 1) )
